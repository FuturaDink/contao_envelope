    "autoload": {
        "psr-4": {
            "FuturaWeb\\Envelope\\": ""
        }
    },
    "extra": {
        "contao-manager-plugin": "FuturaWeb\\Envelope\\Classes\\Contao\\Manager\\Plugin"
    }
}
